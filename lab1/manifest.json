{
  "name": "EECS1720 Extension", 
  "version": "100.0",
  "description": "Build an Basic Extension!",
  "permissions": ["storage", "activeTab", "scripting"],
  "icons": {
    "16": "img/icon16.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png"
    }
  },
  "options_page": "options.html",
"manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  }
 
}
  


